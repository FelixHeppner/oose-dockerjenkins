{
  "name": "oose-dockerjenkins",
  "version": "0.1.0",
  "author": "Felix Heppner <Felix.Heppner@oose.de>",
  "summary": "Run Jenkins as docker container",
  "license": "Apache-2.0",
  "source": "https://github.com/FelixHeppner/oose-dockerjenkins",
  "project_page": "https://github.com/FelixHeppner/oose-dockerjenkins",
  "issues_url": "https://github.com/FelixHeppner/oose-dockerjenkins/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_range": ">= 2.0.0"
    },
    {
      "name": "garethr-docker",
      "version_range": ">= 4.1.1"
    },
    {
      "name": "puppetlabs-vcsrepo",
      "version_range": ">= 1.3.1"
    },
    {
      "name": "puppetlabs-git",
      "version_range": ">= 0.4.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
